FIREBASE_DATABASE_URL=https://your-project-id-default-rtdb.firebaseio.com

# Firebase Admin SDK Configuration
# Get service account JSON from Firebase Console > Project Settings > Service Accounts
# Convert the JSON file to a single-line string and paste here
FIREBASE_SERVICE_ACCOUNT_JSON='{"type":"service_account","project_id":"your-project-id",...}'
VITE_FIREBASE_API_KEY=xxxxyyyy
VITE_FIREBASE_AUTH_DOMAIN=xxxxyyyy
VITE_FIREBASE_PROJECT_ID=xxxxyyyy
VITE_FIREBASE_STORAGE_BUCKET=xxxxyyyy
VITE_FIREBASE_MESSAGING_SENDER_ID=xxxxyyyy
VITE_FIREBASE_APP_ID=xxxxyyyy
VITE_FIREBASE_MEASUREMENT_ID=G-xxxxyyyy

# Firebase Realtime Database URL


# Algorand Wallet Address (Issuer)
VITE_APP_WALLET_ADDRESS=YOUR_ALGORAND_WALLET_ADDRESS_HERE

VITE_ALGORAND_NETWORK=testnet
ISSUER_PRIVATE_KEY=SUPER_SECRET_NEVER_SHARE #MAKE SURE THIS FILE IS IN YOUR .gitignore!!!!

# Custom Verification Platform Configuration

# Google Document AI
# Two processors required: one for fraud detection, one for ID parsing
# Endpoint format: https://us-documentai.googleapis.com/v1/projects/{PROJECT_ID}/locations/{LOCATION}/processors/{PROCESSOR_ID}:process

# ID Fraud Detection Processor
ID_FRAUD_ENDPOINT=https://us-documentai.googleapis.com/v1/projects/697008913216/locations/us/processors/476980138dd1fc28:process

# ID Parser Processor (extracts firstName, lastName, etc)
ID_PARSE_ENDPOINT=https://us-documentai.googleapis.com/v1/projects/697008913216/locations/us/processors/YOUR_PARSER_PROCESSOR_ID:process

# Option 1: Path to Google service account credentials JSON file (for local development)
GOOGLE_APPLICATION_CREDENTIALS=./path/to/google-credentials.json

# Option 2: Google credentials as JSON string (preferred for production/deployment)
# Get from Google Cloud Console > IAM & Admin > Service Accounts > Create Key (JSON)
# Copy the entire JSON content and paste as a single line string
# GOOGLE_CREDENTIALS_JSON='{"type":"service_account","project_id":"...","private_key":"...","client_email":"..."}'

# Face Comparison Service
# Options: 'mock' (development), 'aws-rekognition', 'azure-face'
FACE_COMPARISON_PROVIDER=mock
# Confidence threshold for face matching (0.0 - 1.0)
FACE_MATCH_THRESHOLD=0.85

# AWS Rekognition (if using 'aws-rekognition' provider)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key

# Azure Face API (if using 'azure-face' provider)
# AZURE_FACE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
# AZURE_FACE_KEY=your-azure-face-key

# Photo Storage
# Directory for storing ID and selfie photos
PHOTO_STORAGE_DIR=./storage/photos

